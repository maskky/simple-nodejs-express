docker run --name postgres_DB -p 5432:5432 -e POSTGRES_PASSWORD=1234 -d postgres:latest
docker exec -it postgres_DB psql -U postgres

create database test;

\c test

CREATE TABLE cards (
    id              SERIAL,      
    name            varchar(80)   NOT NULL,
    status          boolean       NOT NULL,
    content         varchar(255)  NOT NULL,           
    category        varchar(255)  NOT NULL,
    author          varchar(80)   NOT NULL
);

INSERT INTO cards(name, status, content, category, author)
VALUES ('test_name', 'true', 'test_content', 'test_category', 'test_author')
RETURNING id;

